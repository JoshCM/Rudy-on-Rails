<UserControl x:Class="RoRClient.View.DummySquareUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RoRClient.View"
             mc:Ignorable="d" 
             x:Name="dummySquareUserControl"
             xmlns:v="clr-namespace:RoRClient.View"
             xmlns:vm="clr-namespace:RoRClient.ViewModel"
             d:DesignHeight="50" d:DesignWidth="50 "
             Canvas.Top="{Binding RealY, ElementName=dummySquareUserControl}"
             Canvas.Left="{Binding RealX, ElementName=dummySquareUserControl}"
             >

    <Rectangle
        Width="50"
        Height="50"
        Fill="BROWN"
        Stroke="WHITE"
        StrokeThickness="1" />

</UserControl>
