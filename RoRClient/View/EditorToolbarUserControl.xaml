        <Button Height="100" Width="100">
            <Image Source="/RoRClient;component/Resources/Images/smiley.jpg"></Image>
        </Button>
        <Button Height="100" Width="100">
            <svgc:SvgViewbox Margin="5" Stretch="Uniform" Source="/RoRClient;component/Resources/Images/test.svg"/>
        </Button>
    </StackPanel>


</UserControl>
