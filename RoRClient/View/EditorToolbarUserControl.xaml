        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>

        <Button Height="100" Width="100" BorderBrush="Red" BorderThickness="5" VerticalAlignment="Bottom" HorizontalAlignment="Center" Margin="50,100">
            <Image Source="{Binding bildstring}"></Image>
        </Button>
    </WrapPanel>
</UserControl>
