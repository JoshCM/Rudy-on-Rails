        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
            <Image Source="/RoRClient;component/Resources/Images/railstraight.png"></Image>
        </Button>
        <Button Height="100" Width="100" Margin="10">
