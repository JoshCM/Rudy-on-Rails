<Window x:Class="RoRClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoRClient.View"
        xmlns:local2="clr-namespace:RoRClient.ViewModel"
        mc:Ignorable="d"
        Title="Rudi on Rails" Height="800" Width="600">

    <Window.Resources>
        <DataTemplate DataType="{x:Type local2:EditorViewModel}">
            <local:EditorUserControl />
        </DataTemplate>
    </Window.Resources>
</Window>
